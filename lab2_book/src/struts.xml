<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="default" extends="struts-default">
		<action name="search" class="SearchAction">
			<result name="SUCCESS">/bookinfo.jsp</result>
			<result name="FAILURE">/fail.jsp</result>
		</action>
		<action name="detail" class="DetailAction">
			<result name="SUCCESS">/detail.jsp</result>
			<result name="FAILURE">/fail.jsp</result>
		</action>
		<action name="delete" class="DeleteAction">
			<result name="SUCCESS">/success.jsp</result>
			<result name="FAILURE">/fail.jsp</result>
		</action>
		<action name="insert" class="InsertAction">
			<result name="SUCCESS">/success.jsp</result>
			<result name="FAILURE">/fail.jsp</result>
			<result name="FURTHER">/insertAuthor.jsp</result>
		</action>
		<action name="insertAuthor" class="InsertAuthorAction">
			<result name="SUCCESS">/success.jsp</result>
			<result name="FAILURE">/fail.jsp</result>
		</action>
		<action name="update" class="UpdateBookAction">
			<result name="SUCCESS">/success.jsp</result>
			<result name="FAILURE">/fail.jsp</result>
		</action>
	</package>
</struts>
